{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"subcommand": "run",
			"problemMatcher": ["$rustc"],
			"group": {
				"isDefault": true,
				"kind": "build",
			},
			"label": "Rust: cargo run - topdown-test"
		},
		{
			"type": "shell",
			"command": "cargo",
			"args": ["run", "--release"],
			"problemMatcher": ["$rustc"],
			"group": "build",
			"label": "Rust: cargo run --release - topdown-test"
		}
	]
}
